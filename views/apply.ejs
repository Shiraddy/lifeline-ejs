                    <input
                      type="radio"
                      class="btn-check"
                      name="studentStatus"
                      value="Graduate"
                      id="vbtn-radio2"
                      autocomplete="off"
                    />
                    <label
                      id="gradbtn"
                      class="btn btn-outline-success px-lg-5 px-sm-4"
                      for="vbtn-radio2"
                      >Graduate</label
                    >
                  </div>
                </div>
                <br />

                <!-- UNDERGRADUATE -->
                <div id="undergrad">
                  <div class="row my-lg-4">
                    <div class="col-lg-6">
                      <label class="label" for="school_attending"
                        >School Attending</label
                      >
                      <input
                        class="apply-input"
                        type="text"
                        name="school_attending"
                      />
                    </div>

                    <div class="col-lg-6">
                      <label class="label" for="level">Level</label>
                      <input
                        class="apply-input"
                        type="text"
                        name="level"
                        placeholder="Level"
                      />
                    </div>
                  </div>

                  <div class="row my-lg-4">
                    <div class="col-lg-6">
                      <label class="label" for="highest_level_of_education"
                        >Certificate Pursuing</label
                      >
                      <select class="apply-input" name="certPurs">
                        <option value="">Select Certificate</option>
                        <option value="">Master</option>
                        <option value="">Degree</option>
                        <option value="">Diploma</option>
                        <option value="">HND</option>
                        <option value="">Certificate</option>
                      </select>
                    </div>

                    <div class="col-lg-6">
                      <label class="label" for="course_studying"
                        >Course Studying</label
                      >
                      <input class="apply-input" type="text" name="coursing" />
                    </div>
                  </div>
                </div>

                <!-- POSTGRADUATES -->
                <div id="grad">
                  <div class="row my-lg-4">
                    <div class="col-lg-6">
                      <label class="label" for="schoolAtt"
                        >School Completed</label
                      >
                      <input
                        class="apply-input"
                        type="text"
                        name="schoolCompleted"
                      />
                    </div>

                    <div class="col-lg-6">
                      <label class="label" for="DoC">Year of Completion</label>
                      <input class="apply-input" type="number" name="YoC" />
                    </div>
                  </div>

                  <div class="row my-lg-4">
                    <div class="col-lg-6">
                      <label class="label" for="course_studied"
                        >Course Studied</label
                      >
                      <input class="apply-input" type="text" name="coursed" />
                    </div>

                    <div class="col-lg-6">
                      <label class="label" for="highest_level_of_education"
                        >Certificate Attained</label
                      >
                      <select class="apply-input" name="cAt">
                        <option value="">Select Certificate</option>
                        <option value="">Masters</option>
                        <option value="">Degree</option>
                        <option value="">Diploma</option>
                        <option value="">HND</option>
                        <option value="">Certificate</option>
                      </select>
                    </div>
                  </div>
                </div>
              </section>

              <div class="d-lg-flex justify-content-end py-3">
                <button
                  id="prevOne"
                  class="mt-lg-5 me-lg-3 btn btn-danger prev-section"
                  type="button"
                >
                  <i class="fa fa-arrow-circle-o-left"></i>
                </button>
                <button
                  id="nextTwo"
                  class="mt-lg-5 btn btn-success me-lg-4 next-section"
                  type="button"
                >
                  <i class="fa fa-arrow-circle-o-right"></i>
                </button>
              </div>
            </fieldset>

