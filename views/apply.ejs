    <div id="tutorApplication" class="container-apply services-apply">
      <section class="">
        <h3 class="apply-header">APPLICATION FORM</h3>
        <div class="form-apply">
          <form action="/apply" method="post" id="multi-section-form">
            <!-- LOGIN DETAILS -->
            <fieldset id="login" class="form-section" data-section="1">
              <section class="login pt-sm-5">
                <legend class="legend me-auto">Create Login Details</legend>

                <div class="row my-lg-3 my-sm-3">
                  <div class="col-lg-12">
                    <label class="label" for="email">Email Address</label>
                    <input
                      class="apply-input"
                      type="email"
                      name="email"
                      required
                    />
                    <p class="error-message text-danger lh-1">
                      Please enter a valid email address
                    </p>
                  </div>
                </div>

                <div class="row">
                  <div class="col-lg-6 my-sm-3">
                    <label class="label" for="password">Password</label>
                    <input
                      required
                      class="apply-input"
                      type="password"
                      name="password"
                    />
                    <p class="error-message text-danger lh-1">
                      This field is required
                    </p>
                  </div>

                  <div class="col-lg-6 my-sm-3">
                    <label class="label" for="altpassword"
                      >Confirm Password</label
                    >
                    <input
                      required
                      class="apply-input"
                      type="password"
                      name="password-confirm"
                    />
                    <p class="error-message text-danger lh-1">
                      The Two (2) Passwords do not match
                    </p>
                  </div>
                </div>

                <!-- <input class="apply-input" type="file" name="profilePic" /> -->
                <div class="d-lg-flex justify-content-end py-2 my-lg-2">
                  <button
                    id="btnlog"
                    class="btn btn-success my-lg-4 btn-prev-next me-lg-5 next-section"
                    type="button"
                  >
                    <i class="fa fa-arrow-circle-o-right"></i>
                  </button>
                  <!-- <button class="btn btn-danger py-2" type="reset">
                  Clear Form
                </button> -->
                </div>
              </section>
            </fieldset>

            <!-- PERSONAL INFORMATION -->
            <fieldset id="personal" class="form-section" data-section="2">
              <section class="personal">
                <legend class="legend me-auto">Personal Details</legend>

                <div class="row my-lg-5">
                  <div class="col-lg-6 col-sm-12">
                    <label class="label" for="firstName">First Name</label>
                    <input
                      class="apply-input"
                      name="firstName"
                      type="text"
                      required
                    />

                    <p class="error-message text-danger lh-1">
                      Please enter your First Name
                    </p>
                  </div>

                  <div class="col-lg-6 col-sm-12">
                    <label class="label" for="lastName">Last Name</label>
                    <input
                      class="apply-input"
                      type="text"
                      name="lastName"
                      required
                    />
                    <p class="error-message text-danger lh-1">
                      Please enter your Last Name
                    </p>
                  </div>
                </div>

                <div class="row my-lg-5">
                  <div class="col-lg-6">
                    <label class="label" for="sex">Sex</label>
                    <select name="sex" id="" class="apply-input">
                      <option value="">Select Gender</option>
                      <option value="">MALE</option>
                      <option value="">FEMALE</option>
                    </select>
                    <p class="error-message text-danger lh-1">
                      Select your gender
                    </p>
                  </div>

                  <div class="col-lg-6">
                    <label class="label" for="dateOfBirth">Date of Birth</label>
                    <input
                      class="apply-input"
                      type="date"
                      name="DoB"
                      id=""
                      required
                    />
                    <p class="error-message text-danger lh-1">
                      Enter your Date of Birth
                    </p>
                  </div>
                </div>

                <div class="row my-lg-5">
                  <div class="col-lg-6">
                    <label class="label" for="contact">Contact</label>
                    <input
                      class="apply-input"
                      type="tel"
                      name="contact"
                      required
                    />
                    <p class="error-message text-danger lh-1">
                      Enter your Phone Number
                    </p>
                  </div>

                  <div class="col-lg-6">
                    <label class="label" for="altContact"
                      >Emergency Contact</label
                    >
                    <input
                      class="apply-input"
                      type="tel"
                      name="emergency"
                      required
                    />
                    <p class="error-message text-danger lh-1">
                      Enter an Emergency Contact Number
                    </p>
                  </div>

                  <div class="col-lg-12 shadow-two mt-lg-3 py-2">
                    <div class="row py-3">
                      <div class="col-lg-5 py-2">
                        <image id="profilePic" src="images/Pic.png" />
                      </div>
                      <input
                        id="imageUpload"
                        type="file"
                        name="profilePic"
                        placeholder="Photo"
                        required=""
                        capture
                      />
                      <div class="col-lg-7">
                        <div class="shadow-two py-2">
                          <h6 class="fs-6">PROFILE PICTURE</h6>

                          <ol>
                            <li class="text-center ps-2">
                              Click on the picture icon
                            </li>
                            <li class="text-center ps-2">
                              Choose a recent picture of yourself
                            </li>
                            <li class="text-center ps-2">
                              Click open to see a preview
                            </li>
                          </ol>
                          <small class="text-dark-50"
                            >NB: Picture Size should not be more than 400
                            kb</small
                          >
                        </div>
                      </div>
                    </div>
                  </div>
                </div>

                <!-- <input class="apply-input" type="file" name="profilePic" /> -->
                <div class="d-lg-flex justify-content-end my-lg-4">
                  <button
                    id="prevlog"
                    class="btn btn-danger my-lg-4 btn-prev-next me-3 prev-section"
                    type="button"
                  >
                    <i class="fa fa-arrow-circle-o-left"></i>
                  </button>
                  <button
