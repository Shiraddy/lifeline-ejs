                    class="btn btn-success my-lg-4 btn-prev-next me-lg-5 next-section"
                    type="button"
                  >
                    <i class="fa fa-arrow-circle-o-right"></i>
                  </button>
                  <!-- <button class="btn btn-danger py-2" type="reset">
                  Clear Form
                </button> -->
                </div>
              </section>
            </fieldset>

            <!-- EDUCATION -->
            <fieldset id="education" class="form-section" data-section="3">
              <section class="education">
                <legend class="legend">Education & Qualification</legend>
                <h6>Select Your Student Status</h6>
                <div>
